<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kg.nurzhamal.quizapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApp"
        android:name="kg.kurmanjan.quizapp.QuizApp">
        <activity android:name="kg.kurmanjan.quizapp.QuestionsActivity">
        </activity>
        <activity android:name="kg.kurmanjan.quizapp.SplashActivity"></activity>
        <activity android:name="kg.kurmanjan.quizapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>